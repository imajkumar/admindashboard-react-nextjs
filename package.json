{
  "name": "adminfront",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run build && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome check",
    "format": "biome format --write",
    "type-check": "tsc --noEmit",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "biome format --write",
      "biome check"
    ],
    "*.{json}": [
      "biome format --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@ant-design/nextjs-registry": "^1.1.0",
    "@types/handlebars": "^4.0.40",
    "@types/nodemailer": "^7.0.1",
    "antd": "^5.15.0",
    "axios": "^1.11.0",
    "handlebars": "^4.7.8",
    "next": "^14.2.5",
    "nodemailer": "^7.0.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
